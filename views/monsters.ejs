<%- include('header.ejs') %>
<header>
	<h1 class="loud-voice">Monsters Page</h1>
</header>

<section>
	<inner-column>
		<ul>
			<% monsters.forEach( function(monster) { %>
			<li>
				<a href="monster/<%= monster.slug%>"><%= monster.name%></a>
			</li>
			<% }); %>
		</ul>
	</inner-column>
</section>